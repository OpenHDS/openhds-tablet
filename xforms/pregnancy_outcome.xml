      <instance>
        <pregnancy_outcome id="pregnancy_outcome">
          <start/>
          <end/>
          <deviceid/>
          <motherId/>
          <fatherId/>
          <visitId/>
          <fieldWorkerId/>
          <nboutcomes/>
          <recordedDate/>
          <partialDate/>
          <outcomes_count/>
          <outcomes jr:template="">
            <outcomeType/>
            <childId/>
            <socialGroupId/>
            <firstName/>
            <middleName/>
            <lastName/>
            <gender/>
            <ordinalPosition/>
            <relationshipToGroupHead/>
          </outcomes>
          <birthingPlace/>
          <birthingPlaceOther/>
          <totalNumberChildrenStillLiving/>
          <attendedANC/>
          <processedByMirth/>
          <meta>
            <instanceID/>
          </meta>
        </pregnancy_outcome>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/pregnancy_outcome/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/pregnancy_outcome/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/pregnancy_outcome/deviceid" type="string"/>
      <bind nodeset="/pregnancy_outcome/motherId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_outcome/fatherId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_outcome/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_outcome/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_outcome/nboutcomes" readonly="true()" required="true()" type="int"/>
      <bind nodeset="/pregnancy_outcome/recordedDate" required="true()" type="date"/>
      <bind nodeset="/pregnancy_outcome/partialDate" required="true()" type="select1"/>
      <bind calculate=" /pregnancy_outcome/nboutcomes " nodeset="/pregnancy_outcome/outcomes_count" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_outcome/outcomes/outcomeType" required="true()" type="select1"/>
      <bind nodeset="/pregnancy_outcome/outcomes/childId" readonly="true()" relevant="selected( /pregnancy_outcome/outcomes/outcomeType , 'LBR')" required="true()" type="string"/>
      <bind nodeset="/pregnancy_outcome/outcomes/socialGroupId" readonly="true()" relevant="selected( /pregnancy_outcome/outcomes/outcomeType , 'LBR')" required="true()" type="string"/>
      <bind nodeset="/pregnancy_outcome/outcomes/firstName" relevant="selected( /pregnancy_outcome/outcomes/outcomeType , 'LBR')" required="true()" type="string"/>
      <bind nodeset="/pregnancy_outcome/outcomes/middleName" relevant="selected( /pregnancy_outcome/outcomes/outcomeType , 'LBR')" type="string"/>
      <bind nodeset="/pregnancy_outcome/outcomes/lastName" relevant="selected( /pregnancy_outcome/outcomes/outcomeType , 'LBR')" required="true()" type="string"/>
      <bind nodeset="/pregnancy_outcome/outcomes/gender" relevant="selected( /pregnancy_outcome/outcomes/outcomeType , 'LBR')" required="true()" type="select1"/>
      <bind nodeset="/pregnancy_outcome/outcomes/ordinalPosition" relevant="selected( /pregnancy_outcome/outcomes/outcomeType , 'LBR')" required="true()" type="int"/>
      <bind nodeset="/pregnancy_outcome/outcomes/relationshipToGroupHead" relevant="selected( /pregnancy_outcome/outcomes/outcomeType , 'LBR')" required="true()" type="select1"/>
      <bind nodeset="/pregnancy_outcome/birthingPlace" required="true()" type="select1"/>
      <bind nodeset="/pregnancy_outcome/birthingPlaceOther" relevant="selected( /pregnancy_outcome/birthingPlace , 'OTHER')" required="true()" type="string"/>
      <bind nodeset="/pregnancy_outcome/totalNumberChildrenStillLiving" required="true()" type="int"/>
      <bind nodeset="/pregnancy_outcome/attendedANC" required="true()" type="select1"/>
      <bind nodeset="/pregnancy_outcome/processedByMirth" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/pregnancy_outcome/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
