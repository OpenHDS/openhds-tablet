      <instance>
        <in_migration id="in_migration">
          <start/>
          <end/>
          <deviceid/>
          <earliestDate/>
          <openhds>
            <visitId/>
            <fieldWorkerId/>
            <migrationType/>
            <locationId/>
          </openhds>
          <individualInfo>
            <individualId/>
            <motherId/>
            <fatherId/>
            <firstName/>
            <middleName/>
            <lastName/>
            <gender/>
            <dateOfBirth/>
            <partialDate/>
            <religion/>
          </individualInfo>
          <dateOfMigration/>
          <warning/>
          <origin/>
          <reason/>
          <maritalChange/>
          <reasonOther/>
          <movedfrom/>
          <shortorlongstay/>
          <processedByMirth/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </in_migration>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/in_migration/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/in_migration/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/in_migration/deviceid" type="string"/>
      <bind nodeset="/in_migration/earliestDate" readonly="true()" type="date"/>
      <bind nodeset="/in_migration/openhds/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/in_migration/openhds/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/in_migration/openhds/migrationType" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/in_migration/openhds/locationId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/in_migration/individualInfo/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/in_migration/individualInfo/motherId" readonly="true()" relevant=" /in_migration/openhds/migrationType  = 'EXTERNAL_INMIGRATION'" required="true()" type="string"/>
      <bind nodeset="/in_migration/individualInfo/fatherId" readonly="true()" relevant=" /in_migration/openhds/migrationType  = 'EXTERNAL_INMIGRATION'" required="true()" type="string"/>
      <bind nodeset="/in_migration/individualInfo/firstName" relevant=" /in_migration/openhds/migrationType  = 'EXTERNAL_INMIGRATION'" required="true()" type="string"/>
      <bind nodeset="/in_migration/individualInfo/middleName" relevant=" /in_migration/openhds/migrationType  = 'EXTERNAL_INMIGRATION'" type="string"/>
      <bind nodeset="/in_migration/individualInfo/lastName" relevant=" /in_migration/openhds/migrationType  = 'EXTERNAL_INMIGRATION'" required="true()" type="string"/>
      <bind nodeset="/in_migration/individualInfo/gender" relevant=" /in_migration/openhds/migrationType  = 'EXTERNAL_INMIGRATION'" required="true()" type="select1"/>
      <bind constraint=". &lt; today()" nodeset="/in_migration/individualInfo/dateOfBirth" relevant=" /in_migration/openhds/migrationType  = 'EXTERNAL_INMIGRATION'" required="true()" type="date"/>
      <bind nodeset="/in_migration/individualInfo/partialDate" relevant=" /in_migration/openhds/migrationType  = 'EXTERNAL_INMIGRATION'" required="true()" type="select1"/>
      <bind nodeset="/in_migration/individualInfo/religion" relevant=" /in_migration/openhds/migrationType  = 'EXTERNAL_INMIGRATION'" required="true()" type="select1"/>
      <bind constraint=". &lt;= today() and . &gt;  /in_migration/earliestDate " jr:constraintMsg="Date of migration can not be in the future" nodeset="/in_migration/dateOfMigration" required="true()" type="date"/>
      <bind nodeset="/in_migration/warning" readonly="true()" relevant=" /in_migration/individualInfo/dateOfBirth &gt;= /in_migration/dateOfMigration  and ( /in_migration/openhds/migrationType  = 'EXTERNAL_INMIGRATION')" required="true()" type="string"/>
      <bind nodeset="/in_migration/origin" relevant=" /in_migration/openhds/migrationType  = 'INTERNAL_INMIGRATION'" required="true()" type="string"/>
      <bind nodeset="/in_migration/reason" required="true()" type="select1"/>
      <bind nodeset="/in_migration/maritalChange" relevant="../reason = 'MARITAL_CHANGE'" required="true()" type="select1"/>
      <bind nodeset="/in_migration/reasonOther" relevant="../reason = 'OTHER'" required="true()" type="string"/>
      <bind nodeset="/in_migration/movedfrom" relevant=" /in_migration/openhds/migrationType  = 'EXTERNAL_INMIGRATION'" required="true()" type="select1"/>
      <bind nodeset="/in_migration/shortorlongstay" required="true()" type="select1"/>
      <bind nodeset="/in_migration/processedByMirth" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/in_migration/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat( /in_migration/individualInfo/individualId ,'_', /in_migration/openhds/migrationType ,'_', /in_migration/openhds/visitId ,'_', /in_migration/openhds/fieldWorkerId )" nodeset="/in_migration/meta/instanceName" type="string"/>
    </model>
  </h:head>
