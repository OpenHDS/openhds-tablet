      <instance>
        <death_to_hoh id="death_to_hoh">
          <start/>
          <end/>
          <deviceid/>
          <earliestDate/>
          <openhds>
            <visitId/>
            <fieldWorkerId/>
            <householdId/>
            <individualId/>
            <firstName/>
            <lastName/>
            <new_hoh_id/>
          </openhds>
          <date/>
          <diagnoseddeath/>
          <whom/>
          <causeofdeathdiagnosed/>
          <causofdeathnotdiagnosed/>
          <placeOfDeath/>
          <OtherplaceOfDeath/>
          <causeOfDeath/>
          <hh_people_nb/>
          <membershiptonewhoh_count/>
          <membershiptonewhoh jr:template="">
            <extId/>
            <memberName/>
            <socialGroupId/>
            <relationshipToGroupHead/>
          </membershiptonewhoh>
          <processedByMirth/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </death_to_hoh>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/death_to_hoh/start" readonly="true()" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/death_to_hoh/end" readonly="true()" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/death_to_hoh/deviceid" readonly="true()" type="string"/>
      <bind nodeset="/death_to_hoh/earliestDate" readonly="true()" type="date"/>
      <bind nodeset="/death_to_hoh/openhds/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/death_to_hoh/openhds/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/death_to_hoh/openhds/householdId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/death_to_hoh/openhds/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/death_to_hoh/openhds/firstName" readonly="true()" type="string"/>
      <bind nodeset="/death_to_hoh/openhds/lastName" readonly="true()" type="string"/>
      <bind nodeset="/death_to_hoh/openhds/new_hoh_id" readonly="true()" required="true()" type="string"/>
      <bind constraint=". &lt; today() and . &gt; /death_to_hoh/earliestDate " jr:constraintMsg="Date of death can not be in the future or before the earliest date" nodeset="/death_to_hoh/date" required="true()" type="date"/>
      <bind nodeset="/death_to_hoh/diagnoseddeath" required="true()" type="select1"/>
      <bind nodeset="/death_to_hoh/whom" relevant="selected( /death_to_hoh/diagnoseddeath , '1')" required="true()" type="select1"/>
      <bind nodeset="/death_to_hoh/causeofdeathdiagnosed" relevant="selected( /death_to_hoh/diagnoseddeath , '1')" required="true()" type="select1"/>
      <bind nodeset="/death_to_hoh/causofdeathnotdiagnosed" relevant="selected( /death_to_hoh/diagnoseddeath , '2')" required="true()" type="select1"/>
      <bind nodeset="/death_to_hoh/placeOfDeath" required="true()" type="select1"/>
      <bind nodeset="/death_to_hoh/OtherplaceOfDeath" relevant="selected( /death_to_hoh/placeOfDeath , 'OTHER')" required="true()" type="string"/>
      <bind calculate="concat( /death_to_hoh/causeofdeathdiagnosed , /death_to_hoh/causofdeathnotdiagnosed )" nodeset="/death_to_hoh/causeOfDeath" readonly="true()" type="string"/>
      <bind nodeset="/death_to_hoh/hh_people_nb" readonly="true()" relevant="false()" required="true()" type="int"/>
      <bind calculate=" /death_to_hoh/hh_people_nb " nodeset="/death_to_hoh/membershiptonewhoh_count" readonly="true()" type="string"/>
      <bind nodeset="/death_to_hoh/membershiptonewhoh/extId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/death_to_hoh/membershiptonewhoh/memberName" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/death_to_hoh/membershiptonewhoh/socialGroupId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/death_to_hoh/membershiptonewhoh/relationshipToGroupHead" required="true()" type="select1"/>
      <bind nodeset="/death_to_hoh/processedByMirth" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/death_to_hoh/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat( /death_to_hoh/openhds/individualId ,'_DTH_HOH_', /death_to_hoh/openhds/visitId ,'_', /death_to_hoh/openhds/fieldWorkerId )" nodeset="/death_to_hoh/meta/instanceName" type="string"/>
    </model>
  </h:head>
