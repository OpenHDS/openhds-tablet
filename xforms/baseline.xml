      <instance>
        <baseline id="baseline">
          <start/>
          <end/>
          <deviceId/>
          <openhds>
            <migrationType/>
            <locationId/>
            <visitId/>
            <fieldWorkerId/>
          </openhds>
          <individualInfo>
            <individualId/>
            <motherId/>
            <fatherId/>
            <firstName/>
            <middleName/>
            <lastName/>
            <gender/>
            <dateOfBirth/>
            <partialDate/>
            <religion/>
          </individualInfo>
          <dateOfMigration/>
          <warning/>
          <visitDate/>
          <majo4mo/>
          <spelasni/>
          <processedByMirth/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </baseline>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/baseline/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/baseline/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/baseline/deviceId" type="string"/>
      <bind nodeset="/baseline/openhds/migrationType" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/baseline/openhds/locationId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/baseline/openhds/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/baseline/openhds/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/baseline/individualInfo/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/baseline/individualInfo/motherId" readonly="true()" relevant=" /baseline/openhds/migrationType  = 'BASELINE'" required="true()" type="string"/>
      <bind nodeset="/baseline/individualInfo/fatherId" readonly="true()" relevant=" /baseline/openhds/migrationType  = 'BASELINE'" required="true()" type="string"/>
      <bind nodeset="/baseline/individualInfo/firstName" relevant=" /baseline/openhds/migrationType  = 'BASELINE'" required="true()" type="string"/>
      <bind nodeset="/baseline/individualInfo/middleName" relevant=" /baseline/openhds/migrationType  = 'BASELINE'" type="string"/>
      <bind nodeset="/baseline/individualInfo/lastName" relevant=" /baseline/openhds/migrationType  = 'BASELINE'" required="true()" type="string"/>
      <bind nodeset="/baseline/individualInfo/gender" relevant=" /baseline/openhds/migrationType  = 'BASELINE'" required="true()" type="select1"/>
      <bind constraint=". &lt; today()" nodeset="/baseline/individualInfo/dateOfBirth" relevant=" /baseline/openhds/migrationType  = 'BASELINE'" required="true()" type="date"/>
      <bind nodeset="/baseline/individualInfo/partialDate" relevant=" /baseline/openhds/migrationType  = 'BASELINE'" required="true()" type="select1"/>
      <bind nodeset="/baseline/individualInfo/religion" relevant=" /baseline/openhds/migrationType  = 'BASELINE'" required="true()" type="select1"/>
      <bind constraint=". &lt;= today()" jr:constraintMsg="the residency date can not be in the future" nodeset="/baseline/dateOfMigration" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/baseline/warning" readonly="true()" relevant=" /baseline/individualInfo/dateOfBirth &gt; /baseline/dateOfMigration  and ../migrationType = 'BASELINE'" required="true()" type="string"/>
      <bind constraint=". &lt;= today()" jr:constraintMsg="the visit date can not be in the future" nodeset="/baseline/visitDate" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/baseline/majo4mo" required="true()" type="select1"/>
      <bind nodeset="/baseline/spelasni" required="true()" type="select1"/>
      <bind nodeset="/baseline/processedByMirth" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/baseline/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat( /baseline/individualInfo/individualId ,'_BASELINE_', /baseline/openhds/visitId ,'_', /baseline/openhds/fieldWorkerId )" nodeset="/baseline/meta/instanceName" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="field-list" ref="/baseline/openhds">
      <label ref="jr:itext('/baseline/openhds:label')"/>
      <input ref="/baseline/openhds/migrationType">
        <label ref="jr:itext('/baseline/openhds/migrationType:label')"/>
      </input>
      <input ref="/baseline/openhds/locationId">
        <label ref="jr:itext('/baseline/openhds/locationId:label')"/>
      </input>
      <input ref="/baseline/openhds/visitId">
        <label ref="jr:itext('/baseline/openhds/visitId:label')"/>
      </input>
      <input ref="/baseline/openhds/fieldWorkerId">
        <label ref="jr:itext('/baseline/openhds/fieldWorkerId:label')"/>
      </input>
    </group>
    <group ref="/baseline/individualInfo">
