        </item>
      </select1>
      <input ref="/baseline/individualInfo/dateOfBirth">
        <label ref="jr:itext('/baseline/individualInfo/dateOfBirth:label')"/>
      </input>
      <select1 ref="/baseline/individualInfo/partialDate">
        <label ref="jr:itext('/baseline/individualInfo/partialDate:label')"/>
