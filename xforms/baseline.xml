      </select1>
      <select1 ref="/baseline/individualInfo/religion">
        <label ref="jr:itext('/baseline/individualInfo/religion:label')"/>
        <item>
          <label ref="jr:itext('/baseline/individualInfo/religion/1:label')"/>
          <value>1</value>
        </item>
        <item>
          <label ref="jr:itext('/baseline/individualInfo/religion/2:label')"/>
          <value>2</value>
        </item>
        <item>
