      <instance>
        <change_hoh id="change_hoh">
          <start/>
          <end/>
          <deviceid/>
          <openhds>
            <visitId/>
            <visitDate/>
            <fieldWorkerId/>
            <householdId/>
            <householdName/>
            <individualId/>
            <new_hoh_id/>
            <new_hoh_name/>
          </openhds>
          <date/>
          <hh_people_nb/>
          <membershiptonewhoh_count/>
          <membershiptonewhoh jr:template="">
            <extId/>
            <memberName/>
            <socialGroupId/>
            <relationshipToGroupHead/>
          </membershiptonewhoh>
          <processedByMirth/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </change_hoh>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/change_hoh/start" readonly="true()" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/change_hoh/end" readonly="true()" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/change_hoh/deviceid" readonly="true()" type="string"/>
      <bind nodeset="/change_hoh/openhds/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_hoh/openhds/visitDate" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/change_hoh/openhds/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_hoh/openhds/householdId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_hoh/openhds/householdName" readonly="true()" type="string"/>
      <bind nodeset="/change_hoh/openhds/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_hoh/openhds/new_hoh_id" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_hoh/openhds/new_hoh_name" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_hoh/date" required="true()" type="date"/>
      <bind nodeset="/change_hoh/hh_people_nb" readonly="true()" relevant="false()" required="true()" type="int"/>
      <bind calculate=" /change_hoh/hh_people_nb " nodeset="/change_hoh/membershiptonewhoh_count" readonly="true()" type="string"/>
      <bind nodeset="/change_hoh/membershiptonewhoh/extId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_hoh/membershiptonewhoh/memberName" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_hoh/membershiptonewhoh/socialGroupId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_hoh/membershiptonewhoh/relationshipToGroupHead" required="true()" type="select1"/>
      <bind nodeset="/change_hoh/processedByMirth" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/change_hoh/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;CHANGE_HOH-&quot;, /change_hoh/openhds/householdName )" nodeset="/change_hoh/meta/instanceName" type="string"/>
    </model>
  </h:head>
